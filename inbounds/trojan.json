{
    "listen": "0.0.0.0",
    "port": 16789,
    "protocol": "trojan",
    "settings": {
        "clients": [
            {
                "password": "",
                "flow": "xtls-rprx-direct"
            }
        ],
        "fallbacks": [
            {
                "dest": "/dev/shm/default.sock",
                "xver": 1
            },
            {
                "alpn": "h2",
                "dest": "/dev/shm/h2c.sock",
                "xver": 1
            }
        ]
    },
    "streamSettings": {
        "network": "tcp",
        "security": "xtls",
        "xtlsSettings": {
            "alpn": [
                "http/1.1",
                "h2"
            ],
            "certificates": [
                {
                    "certificateFile": "/usr/local/etc/xray/cert.pem",
                    "keyFile": "/usr/local/etc/xray/key.pem",
                    "ocspStapling": 3600
                }
            ],
            "minVersion": "1.2"
        }
    }
}