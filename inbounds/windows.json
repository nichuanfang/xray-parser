{
    "listen": "0.0.0.0",
    "port": 443,
    "protocol": "vless",
    "settings": {
        "clients": [
            {
                "id": "",
                "flow": "xtls-rprx-vision"
            }
        ],
        "decryption": "none",
        "fallbacks": [
            {
                "dest": "nginx:8080"
            },
            {
                "alpn": "h2",
                "dest": "nginx:8081"
            }
        ]
    },
    "streamSettings": {
        "network": "tcp",
        "security": "tls",
        "tlsSettings": {
            "serverName": "",
            "alpn": [
                "h2",
                "http/1.1"
            ],
            "certificates": [
                {
                    "ocspStapling": 3600,
                    "certificateFile": "/usr/local/etc/xray/cert/cert.pem",
                    "keyFile": "/usr/local/etc/xray/cert/key.pem"
                }
            ]
        }
    },
    "sniffing": {
        "enabled": true,
        "destOverride": [
            "http",
            "tls"
        ]
    }
}