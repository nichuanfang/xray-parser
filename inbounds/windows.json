{
  "listen": "0.0.0.0",
  "port": 0,
  "protocol": "vless",
  "settings": {
    "clients": [
      {
        "id": "",
        "flow": "xtls-rprx-vision"
      }
    ],
    "decryption": "none",
    "fallbacks": [
      {
        "dest": "5001",
        "xver": 1
      },
      {
        "alpn": "h2",
        "dest": "5002",
        "xver": 1
      }
    ]
  },
  "streamSettings": {
    "network": "tcp",
    "security": "tls",
    "tlsSettings": {
      "rejectUnknownSni": true,
      "minVersion": "1.2",
      "certificates": [
        {
          "ocspStapling": 3600,
          "certificateFile": "/usr/local/bin/xray/cert/cert.pem",
          "keyFile": "/usr/local/bin/xray/cert/key.pem"
        }
      ],
      "alpn": [
        "h2",
        "http/1.1"
      ]
    },
    "sockopt": {
      "mtu": 1350,
      "tcpFastOpen": true
    }
  },
  "sniffing": {
    "enabled": true,
    "destOverride": [
      "http",
      "tls",
      "quic"
    ]
  }
}
