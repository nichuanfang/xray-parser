{
	"log": {
		"loglevel": "warning",
		"dnsLog": false
	},
	"inbounds": [{
			"tag": "socks-in",
			"protocol": "socks",
			"listen": "127.0.0.1",
			"port": 10808,
			"settings": {
				"udp": true
			}
		},
		{
			"tag": "http-in",
			"protocol": "http",
			"listen": "127.0.0.1",
			"port": 10809
		}
	],
	"outbounds": [{
			"tag": "proxy",
			"protocol": "vless",
			"settings": {
				"vnext": [{
					"address": "xray服务器ip",
					"port": 0,
					"users": [{
						"id": "xray的uuid",
						"encryption": "none",
						"flow": "xtls-rprx-vision",
						"level": 0
					}]
				}]
			},
			"streamSettings": {
				"network": "tcp",
				"security": "tls",
				"tlsSettings": {
					"serverName": "换成你的域名",
					"allowInsecure": false,
					"fingerprint": "chrome",
					"alpn": [
						"h2",
						"http/1.1"
					]
				}
			}
		},
		{
			"tag": "direct",
			"protocol": "freedom",
			"settings": {
        			"domainStrategy": "UseIPv4"
      			}
		},
		{
			"tag": "block",
			"protocol": "blackhole",
			"settings": {
        			"type": "http"
      			}
		}
	],
	"policy": null,
	"dns": {
		"servers": [
			"1.1.1.1",
			"localhost"
		],
		"queryStrategy": "UseIPv4"
	}
}
