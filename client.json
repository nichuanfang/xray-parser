{
    "log": {
        "access": "log/access.log",
        "error": "log/error.log",
        "loglevel": "warning",
        "dnsLog": false
    },
    "inbounds": [
        {
            "tag": "socks",
            "port": 10808,
            "listen": "0.0.0.0",
            "protocol": "socks",
            "sniffing": {
                "enabled": true,
                "destOverride": [
                    "http",
                    "tls"
                ],
                "routeOnly": false
            },
            "settings": {
                "auth": "noauth",
                "udp": true,
                "allowTransparent": false
            }
        },
        {
            "tag": "http",
            "port": 10809,
            "listen": "0.0.0.0",
            "protocol": "http",
            "sniffing": {
                "enabled": true,
                "destOverride": [
                    "http",
                    "tls"
                ],
                "routeOnly": false
            },
            "settings": {
                "auth": "noauth",
                "udp": true,
                "allowTransparent": false
            }
        }
    ],
    "outbounds": [
        {
            "tag": "proxy",
            "protocol": "vless",
            "settings": {
                "vnext": [
                    {
                        "address": "xray服务器ip",
                        "port": 0,
                        "users": [
                            {
                                "id": "xray的uuid",
                                "flow": "xtls-rprx-vision",
                                "alterId": 0,
                                "email": "t@t.tt",
                                "security": "auto",
                                "encryption": "none"
                            }
                        ]
                    }
                ]
            },
            "streamSettings": {
                "network": "grpc",
                "security": "reality",
                "realitySettings": {
                    "show": false,
                    "fingerprint": "chrome",
                    "serverName": "",
                    "publicKey": "xray的公钥",
                    "shortId": "客户端id",
                    "spiderX": "爬虫路径"
                },
                "grpcSettings": {
                    "serviceName": "cinima",
                    "multiMode": false,
                    "idle_timeout": 60,
                    "health_check_timeout": 20,
                    "permit_without_stream": false,
                    "initial_windows_size": 0
                }
            }
        },
        {
            "tag": "direct",
            "protocol": "freedom",
            "settings": {
                "domainStrategy": "UseIPv4"
            }
        },
        {
            "tag": "block",
            "protocol": "blackhole",
            "settings": {
                "response": {
                    "type": "http"
                }
            }
        }
    ],
    "dns": {
        "servers": [
            {
                "address": "8.8.4.4",
                "domains": [
                    "geosite:geolocation-!cn"
                ]
            },
            {
                "address": "223.5.5.5",
                "domains": [
                    "geosite:cn"
                ]
            },
            "localhost"
        ]
    }
}
