{
	"log": {
		"access": "log/access.log",
		"error": "log/error.log",
		"loglevel": "warning",
		"dnsLog": false
	},
	"inbounds": [
		{
			"listen": "127.0.0.1",
			"port": 10808,
			"protocol": "socks"
		},
		{
			"listen": "127.0.0.1",
			"port": 10809,
			"protocol": "http"
		}
	],
	"outbounds": [
		{
			"tag": "proxy",
			"protocol": "vless",
			"settings": {
				"vnext": [
					{
						"address": "xray服务器ip",
						"port": 0,
						"users": [
							{
								"id": "xray的uuid",
								"encryption": "none",
								"flow": "xtls-rprx-vision"
							}
						]
					}
				]
			},
			"streamSettings": {
				"network": "tcp",
				"security": "tls",
				"tlsSettings": {
					"serverName": "换成你的域名",
					"allowInsecure": false,
					"fingerprint": "chrome"
				}
			}
		},
		{
			"tag": "direct",
			"protocol": "freedom"
		},
		{
			"tag": "block",
			"protocol": "blackhole"
		}
	],
	"policy": null
}
