{
	"log": {
		"access": "log/access.log",
		"error": "log/error.log",
		"loglevel": "warning",
		"dnsLog": false
	},
	"inbounds": [
		{
			"listen": "127.0.0.1",
			"port": 10808,
			"protocol": "socks"
		},
		{
			"listen": "127.0.0.1",
			"port": 10809,
			"protocol": "http"
		}
	],
	"outbounds": [
		{
			"tag": "proxy",
			"protocol": "vless",
			"settings": {
				"vnext": [
					{
						"address": "xray服务器ip",
						"port": 0,
						"users": [
							{
								"id": "xray的uuid",
								"encryption": "none",
								"flow": "xtls-rprx-vision"
							}
						]
					}
				]
			},
			"streamSettings": {
				"network": "tcp",
				"security": "tls",
				"tlsSettings": {
					"serverName": "",
					"fingerprint": "chrome"
				}
			}
		},
		{
			"tag": "direct",
			"protocol": "freedom",
			"settings": {
				"domainStrategy": "UseIP"
			}
		},
		{
			"tag": "block",
			"protocol": "blackhole",
			"settings": {
				"response": {
					"type": "http"
				}
			}
		}
	],
	"policy": {
		"levels": {
			"0": {
				"handshake": 4,
				"connIdle": 300,
				"uplinkOnly": 0,
				"downlinkOnly": 0,
				"statsUserUplink": false,
				"statsUserDownlink": false,
				"bufferSize": 512
			}
		},
		"system": {
			"statsInboundUplink": false,
			"statsInboundDownlink": false,
			"statsOutboundUplink": false,
			"statsOutboundDownlink": false
		}
	}
}